<style>
    .ui-menu {
        width: 434.328px !important;
    }

        .ui-menu .ui-menu-item {
            width: 415.328px !important;
        }

    .autocomplete {
        position: relative;
        display: inline-block;
    }

    .g-head-t {
        font-size: 12px;
        padding: 0 12px;
        color: grey;
    }

    .g-amt-t {
        font-size: 15px;
        padding: 0 12px;
        color: #003d66;
        font-weight: 700;
    }

    .spn-Yes1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 10px;
        font-weight: 600;
        background: #28a745;
        color: white;
    }

    .spn-No1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 10px;
        font-weight: 600;
        background: #ef0808;
        color: white;
    }

    .disabled {
        cursor: not-allowed;
    }

    .btn-spn-opt:hover {
        background-color: #80808017;
        border: 1px solid #003d66;
    }

    .btn-spn-opt {
        border: 1px solid #e5e5e5;
        border-radius: 28px;
        padding: 5px 15px;
        float: left;
        margin-bottom: 5px;
        margin-right: 5px;
        transition-duration: 0.3s;
        -webkit-transition: 0.3s;
        cursor: pointer;
        font-size: 11px;
        white-space: nowrap;
        font-weight: 600;
        background: #fff;
        color: #003d66;
        position: relative;
        z-index: 0;
        overflow: hidden;
        margin-top: 5px;
    }

    .btn-spn-opt-active {
        color: #fff !important;
        background-color: #003d66 !important;
    }

    .pwd_field {
        -webkit-text-security: disc;
    }

    #txtCompanyName_suggestion {
        font-size: 11.5px;
    }

    [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak, .ng-hide:not(.ng-hide-animate) {
        display: none !important;
    }

    .img-block {
        display: block;
    }

    .img-none {
        display: none;
    }

    ul {
        list-style: none;
        padding: 0;
        margin: 0;
    }

    #CaratModal .form-control {
        border: 1px solid black;
    }

    .api-filter-popup ul.color-whit-box li {
        display: inline-block;
        margin: 4px 4px;
    }

    ul.color-whit-box li.active {
        background: rgba(0, 61, 102,1);
        color: #fff;
    }

    ul.color-whit-box li {
        padding: 3px 13px;
        -webkit-box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        -moz-box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        margin: 2px 4px;
        -webkit-border-radius: 50px;
        -moz-border-radius: 50px;
        border-radius: 50px;
        cursor: pointer;
        font-size: 14px;
        color: #003d66;
        font-weight: 500;
        background: #fff;
        white-space: nowrap;
        -webkit-transition: all 0.3s;
        -moz-transition: all 0.3s;
        -ms-transition: all 0.3s;
        -o-transition: all 0.3s;
        transition: all 0.3s;
    }

    .custom-box-list p {
        word-break: break-all;
        border: 1px solid #ced4da;
        padding: 0px 4px;
        font-weight: 400;
        color: #495057;
        background-color: #fff;
        background-clip: padding-box;
